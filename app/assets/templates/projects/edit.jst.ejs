<input type="text" class="input-huge" name="title" value="<%= project.escape('title') %>" autofocus="autofocus" onfocus="this.value = this.value;"><br>
<input type="text"  class="input-huge" name="url" value="<%= project.escape('url') %>"><br>
<select name="gitrepo" class="input-huge">
  <% _(Jects.repos).each(function (repo) { %>
    <option><%= repo %></option>
  <% }) %>
</select>
